{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/notifications(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/item/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/post-item",
      "dest": "/api/index.js"
    },
    {
      "src": "/student-info",
      "dest": "/api/index.js"
    },
    {
      "src": "/complete-transaction",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
